class parent():
    def main(self):
        global a,b
        a=int(input())
        b=int(input())
class child1(parent):
    def add(self):
        c=a+b
        print(c)
class child2(parent):
    def mul(self):
        d=a*b
        print(d)
obj=child1()
obj.main()
obj.add()

obj1=child2()
obj1.main()
obj1.mul()